<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="default_dashboard_view">
        <field name="name">Default Dashboard View</field>
        <field name="model">website</field>
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <dashboard>
            	<div class="oe_button_box" name="button_box">
					<button name="switch" type="object" class="oe_stat_button" icon="fa-archive">
           				<field name="cdn_activated" widget="boolean_button"/>
           			</button>
    			</div>
            </dashboard>
        </field>
    </record>
</odoo>
